  title = {Analysis of {{Different Methods}} for {{Wave Generation}} and {{Absorption}} in a {{CFD}}-{{Based Numerical Wave Tank}}},
  author = {Miquel, Adria Moreno and Kamath, Arun and Alagan Chella, Mayilvahanan and Archetti, Renata and Bihs, Hans},
