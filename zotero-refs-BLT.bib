@article{galasso2013validation,
  title = {Validation of Ground-Motion Simulations for Historical Events Using {{MDoF}} Systems},
  author = {Galasso, Carmine and Zhong, Peng and Zareian, Farzin and Iervolino, Iunio and Graves, Robert W.},
