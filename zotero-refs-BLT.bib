@article{akiyama2014long,
  title = {Long-Term Seismic Performance of {{RC}} Structures in an Aggressive Environment: Emphasis on Bridge Piers},
  shorttitle = {Long-Term Seismic Performance of {{RC}} Structures in an Aggressive Environment},
  author = {Akiyama, Mitsuyoshi and Frangopol, Dan M.},
