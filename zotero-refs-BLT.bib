@article{gao2017robust,
  title = {Robust Hex-Dominant Mesh Generation Using Field-Guided Polyhedral Agglomeration},
  author = {Gao, Xifeng and Jakob, Wenzel and Tarini, Marco and Panozzo, Daniele},
